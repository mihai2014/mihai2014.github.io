   ],
   "source": [
    "def cartesian(x,y):\n",
    "    #return np.transpose( [ np.tile(x, len(y)), np.repeat(y, len(x)) ] )\n",
    "    a = np.tile(x, len(y))\n",
    "    b = np.repeat(y, len(x))\n",
    "    m = np.array([a,b])\n",
    "    return m.T\n",
    "cartesian(np.array([1,2,3]),np.array([4,5]))"
   ]
